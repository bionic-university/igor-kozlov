<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    {% stylesheets 'bundles/bionicuniversityunusedcss/css/*' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <title>{% block title %}Test Application{% endblock %}</title>
</head>
<body>
<div id="main">
    {% block sidebar %}
        <div id="header"><a href="{{ path('bionic_university_unusedcss_homepage') }}">Home</a>
        {% if is_granted("ROLE_USER") %}
            <a href="{{ path('bionic_university_unusedcss_test_list') }}">View history</a>
            <a href="{{ path('bionic_university_unusedcss_crawl') }}">Crawl site</a>
            <a href="{{ path('logout') }}">Logout</a>
        {% else %}
        <a href="{{ path('account_register') }}">Register</a>
        <a href="{{ path('login') }}">Login</a>{% endif %}
    </div>
    {% endblock %}


<div id="contentColumn">
    <div class="divider">    </div>
    {% block body %}{% endblock %}
</div>
</div>
<div id="footer"></div>
</body>
</html>